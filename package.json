{
  "name": "putasset",
  "version": "4.0.1",
  "description": "upload asset to release on github",
  "main": "lib/putasset.js",
  "bin": {
    "putasset": "bin/putasset.js"
  },
  "scripts": {
    "test": "tape 'test/**/*.js'",
    "watch": "nodemon --watch lib --watch test -d 0.3 --exec",
    "watch:test": "npm run watch -- npm test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "eslint:lib": "eslint lib test",
    "eslint:bin": "eslint -c .eslint-bin.rc bin",
    "lint": "redrun eslint:*",
    "test:run": "bin/putasset.js --filename ./README.md --repo node-putasset --owner coderaiser --tag \"v`version`\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/node-putasset.git"
  },
  "keywords": [
    "github",
    "release",
    "asset",
    "upload"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-putasset/issues"
  },
  "dependencies": {
    "@octokit/rest": "^16.1.0",
    "checkup": "^1.3.0",
    "mime-types": "^2.1.21",
    "minimist": "^1.2.0",
    "readjson": "^1.1.0",
    "try-catch": "^2.0.0",
    "try-to-catch": "^1.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.9.0",
    "nodemon": "^1.9.2",
    "nyc": "^13.1.0",
    "redrun": "^7.0.0",
    "supertape": "^1.1.1",
    "try-to-tape": "^1.1.0",
    "version-io": "^2.0.1"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
